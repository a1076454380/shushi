/*督查审核系统*/
#dongyi_nav{position: relative;top:44px;width: 100%;overflow: hidden;border-bottom: 1px solid #e2e2e2;background: #FFFFFF;}
#dongyi_nav ul{position: relative;padding: 0px;margin: 0px;width: 100%;height: 50px;padding-left: 14px;overflow: hidden;right: 11px;z-index: 888;}
#dongyi_nav ul li{list-style-type: none;width: 50%;text-align: center;float:left;height: 50px;line-height: 50px;font-size: 14px;display: inline-block;padding: 0px 14px;}
#dongyi_nav ul .default {color:#0C3F77;/*border: 1px solid red;*/}
#down_line{position: relative;top:48px;left:0px;height:2px;width:60%;background: #0C3F77;z-index: 99;}
/*内容*/
.qita{margin-top: 15px;padding: 5px 5px;background: #FFFFFF;}
.qita h3{font-size: 12px;font-weight:600;padding: 5px 5px;border-bottom:1px solid #DDDDDD;height: 35px;}
.qita p{line-height:20px;font-size: 12px;font-weight:600;padding: 5px 5px;height: 250; max-height: 250px;}
/*图片样式*/
.img_info{background: #FFFFFF;border-top:1px solid #DDDDDD ;border-bottom:1px solid #DDDDDD;padding: 5px 5px;overflow: hidden;}
.info_img img{width: 65px;height: 65px;margin-left: 20px;float: left;}
.info_img span{width: 25px;height: 25px;border-radius:50%;}
/*按钮样式*/
.buttom{text-align: center;padding:5px 5px;}
.buttom button{width: 45%;height: 45px;border-radius: 8px;}
.buttom_div button{    font-size: 14px;
    font-weight: 400;
    line-height: 1.42;
    position: relative;
    display: inline-block;
    margin-bottom: 0;
    padding: 6px 12px;
    cursor: pointer;
    -webkit-transition: all;
    transition: all;
    -webkit-transition-timing-function: linear;
    transition-timing-function: linear;
    -webkit-transition-duration: .2s;
    transition-duration: .2s;
    text-align: center;
    vertical-align: top;
    white-space: nowrap;
    color: #333;
    border: 1px solid #ccc;
    border-radius: 3px;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
    background-color: #fff;
    background-clip: padding-box;}


.buttom_div{display: flex;justify-content: center;}
.buttom_div button{margin:.2rem}
.item-num01{width: 1rem;display: flex;align-items: center;font-size: 12px;}
.item-name01{display: flex;flex: 1;align-items: center;font-size: 12px;}
.item-sett01{width: 2rem;display: flex;;align-items: center;font-size: 12px;justify-content: center;}
.item-name01 .title{border: 1px solid gainsboro;border-radius: 10px;padding: 2px 5px;margin-right: 5px;font-size: 12px;}
.Competition-item{border-bottom: 1px solid gainsboro;padding-bottom: 10px;}
.item-sett01 span{border: 1px solid gainsboro;border-radius: 5px;padding:10px 15px;}
